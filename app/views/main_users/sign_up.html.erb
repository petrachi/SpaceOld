<script type="text/javascript">
	$(document).ready(function() {
		jsMergeForm("#sign_forms form", "<%= sign_up_url %>", window.location);
		initializeQtip("#sign_forms form");
	});
</script>


<%= form_tag do %>
	<%= hidden_field_tag :call, :js %>
	
	<%= form_row :first_name %>
	<%= form_row :name %>
	<%= form_row :email %>
	<%= form_row :password %>
	<%= form_row :password_confirmation %>

	<%= js_merge_row %>	
<% end %>


