<%= container do %>
	<%= one_col_row :element => :h2 do %>
		<%= t :'super_user.action_links' %>
	<% end %>
	
	<%= two_col_container :collection => @action_links, :disable=>[:container, :spans] do |action_link| %>
		<%= four_span  do %>
			<%= t :"super_user.action_link.#{ action_link }" %>
		<% end %>
		
		<%= two_span do %>
			<%= link_to t(:go_to), game_url(:action => action_link), :class => :btn, :display => :inline %>
		<% end %>
	<% end %>
<% end %>